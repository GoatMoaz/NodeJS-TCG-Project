<%- include('includes/head.ejs') %>
<link rel="stylesheet" href="/css/main.css">
</head>
<body>
    <%- include('includes/nav.ejs') %>
    <main>
        <% if (prods.length> 0) { %>
            <ul>
                <% prods.forEach(product=> { %>
                    <li>
                        <%= product.title %>
                    </li>
                    <% }) %>
            </ul>
            <% } else { %>
                <h1>No Products Found!</h1>
            <% } %>
    </main>
</body>

</html>